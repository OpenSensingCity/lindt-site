@prefix rp: <http://w3id.org/seas/rp/>.

<voc> a rp:RDFSource ; rp:fileName "voc.ttl" ;
  rp:alias <v1/voc> .

<custom_datatypes> a rp:Resource , rp:RDFSource ; 
  rp:alias <v1/custom_datatypes> ;
  rp:fileName "custom_datatypes.ttl" ;
  rp:representation [ rp:fileName "custom_datatypes.html" ; rp:mediaType "text/html;q=1.0" ] ;
  rp:representation [ rp:fileName "custom_datatypes.js" ;  rp:mediaType "application/javascript;q=0.8" ] .

<v1/DBpediaLengthQueries> a rp:Resource ; 
  rp:representation [ rp:fileName "DBpediaLengthQueries.zip" ;  rp:mediaType "application/zip" ] .

<v1/dbpedia> a rp:Resource ; 
  rp:representation [ rp:fileName "dbpedia.zip" ;  rp:mediaType "application/zip" ] .



